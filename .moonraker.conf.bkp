[server]
host: 0.0.0.0
port: 7125
log_path: ~/klipper_logs
config_path: ~/klipper_config
database_path: ~/.moonraker_database
klippy_uds_address: /tmp/klippy_uds

enable_debug_logging: False
enable_object_processing: True

[authorization]
trusted_clients:
    127.0.0.1
    192.168.0.0/16
    ::1/128
    FE80::/10
cors_domains:
    *.lan
    *.local
    *://my.mainsail.xyz
    *://app.fluidd.xyz

[octoprint_compat]

[history]

[update_manager]
refresh_interval: 24

[update_manager client mainsail]
type: web_beta
repo: meteyou/mainsail
path: ~/mainsail

[update_manager klipper_config]
type: git_repo
origin: th33xitus/klipper_config.git
path: ~/klipper_config
is_system_service: False
